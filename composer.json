{
    "name": "bnhashem/deploy-bitbucket",
    "description": ":package_description",
    "keywords": [
        ":vendor_name",
        "laravel",
        "deploy-bitbucket"
    ],
    "homepage": "https://github.com/BNhashem16/deploy-bitbucket",
    "license": "MIT",
    "authors": [{
        "name": "Ahmed Hashem",
        "email": "me@ahashem.info",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0|^7.0|^6.0|^5.5"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Bnhashem\\DeployBitbucket\\": "src"
        }
    },

    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bnhashem\\DeployBitbucket\\DeployBitbucketServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}